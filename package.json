{
  "name": "dotpath-compile",
  "description": "Dot-path based JavaScript object template compiler.",
  "author": "Aleksandr Tsertkov <tsertkov@gmail.com>",
  "version": "0.1.0",
  "main": "lib/compile.js",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsertkov/dotpath-compile/issues"
  },
  "scripts": {
    "test": "npm run cover-test && npm run jshint",
    "cover-test": "istanbul cover --dir artifacts/coverage test/*",
    "jshint": "jshint lib/ test/ package.json",
    "jsdoc": "jsdoc --private --destination artifacts/jsdoc lib/",
    "dev": "nodemon -w lib/ -w test/ test/compile.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tsertkov/dotpath-compile.git"
  },
  "keywords": [
    "dotpath",
    "dot-path",
    "compiler",
    "compile",
    "template",
    "json"
  ],
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jsdoc": "^4.0.2",
    "jshint": "^2.13.6",
    "nodemon": "^2.0.22"
  },
  "dependencies": {
    "dotpath-resolve": "^0.1.3",
    "lodash": "^4.17.21"
  }
}
